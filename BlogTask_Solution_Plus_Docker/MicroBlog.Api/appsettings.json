{
  "ConnectionStrings": {
    "Default": "Server=YOUR_SQL_SERVER;Database=MicroBlogDb;User Id=YOUR_DB_USER;Password=YOUR_DB_PASSWORD;"
  },
  "BlobConnectionString": "DefaultEndpointsProtocol=https;AccountName=YOUR_ACCOUNT_NAME;AccountKey=YOUR_ACCOUNT_KEY;EndpointSuffix=core.windows.net",
  "JwtSettings": {
    "Issuer": "MicroBlog",
    "Audience": "MicroBlogUsers",
    "Secret": "YourSuperSecretKeyHere",
    "ExpiryMinutes": 60
  },
  "Hangfire": {
    "DashboardPath": "/hangfire",
    "QueueName": "default"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}